<%= panel_with_form_for(@company, 'company', 'Company', remote: true) do |f| %>

    <%= f.text_field(:name, 'Name', 'Company name') %>
    <%= f.text_field(:email, 'Email', 'info@example.com') %>
    <%= f.text_field(:web, 'Website', 'http://') %>
      <%= f.submit "Edit", class: "btn btn-default" %>
  <% end %>
