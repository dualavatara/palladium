<%= panel_with_form_for(@actor, dom_id(@actor), 'New actor', url: project_actors_path(@actor.project.id())) do |f| %>
    <%= f.hidden_field :project_id %>
    <%= f.text_field :name, 'Name', 'Name' %>
    <%= f.text_field :desc, 'Description', 'Description' %>
    <%= f.submit class: "btn btn-default" %>
<% end %>